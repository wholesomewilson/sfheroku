<%= form_for @saccount do |f| %>
  <div class="form-group">
    <label>Name</label>
    <%= f.text_field :name, class:"form-control" %>
  </div>
  <div class="form-group">
    <label>Phone Number</label>
    <%= f.text_field :phone, class:"form-control" %>
  </div>
<%= f.submit 'Submit', class:"btn btn-light"  %>
<% end %>
